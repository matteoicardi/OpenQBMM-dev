FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      moment.02.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -2 0 0 0 0];

internalField       uniform 0;

boundaryField
{
    walls
    {
        type                zeroGradient;
        value               $internalField;
    }
    outlet
    {
        type               zeroGradient;
    }
    inlet1
    {
        type               fixedValue;
        value              uniform 1;
    }
    inlet2
    {
        type               fixedValue;
        value              uniform 1;
    }
    defaultFaces
    {
        type    empty;
    }
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
