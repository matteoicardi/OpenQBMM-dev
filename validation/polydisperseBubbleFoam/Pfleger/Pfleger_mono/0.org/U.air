FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type                fixedValue;
        value               uniform (0 0.03 0);
    }
    outlet
    {
        type                inletOutlet;
        phi                 phi.air;
        inletValue          uniform (0 0 0);
        value               $internalField;
    }
    walls
    {
        type                noSlip;
    }
    defaultFaces
    {
        type                empty;
    }
}

